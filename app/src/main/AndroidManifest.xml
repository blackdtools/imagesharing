<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.blackdevelopers.imagesharingtest">
    <application
        android:allowBackup="false"
        android:fullBackupContent="false"
        android:name="net.blackdevelopers.imagesharingtest.ExampleApp"
        android:icon="@mipmap/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ImageSharingStyle">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ImageSharingStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.blackdevelopers.imagesharing.ImageSharingActivity"
            android:theme="@style/Theme.ImageSharingStyle"
            />
    </application>
</manifest>